{
    "name": "zicht/messages-bundle",
    "description": "Zicht Messages bundle",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.1",
        "sonata-project/admin-bundle": "^3.35",
        "symfony/yaml": "^4.4",
        "symfony/finder": "^4.4",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "suggest": {
        "google/cloud-translate": "To use the TranslateCommand with batch-translations through the Google Translate API"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\MessagesBundle\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zicht\\Bundle\\MessagesBundle\\": [
                "tests/"
            ]
        }
    },
    "license": "MIT"
}
